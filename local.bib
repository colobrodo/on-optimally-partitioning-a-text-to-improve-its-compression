	@article{BoVCDSD,
  author   = {Paolo Boldi and Sebastiano Vigna},
  title    = {Complexity of Deciding Sense of Direction},
  journal  = SIAM,
  year     = 2000,
  volume   = 29,
  number   = 3,
  pages    = {779--789},
  category = journal,
  notextra = {
              <p>In this paper we prove that deciding whether a distributed system
              (represented as a coloured digraph with <i>n</i> nodes) has weak sense
              of direction is in AC<sup>1</sup> (using <i>n</i><sup>6</sup> processors). Moreover, we show that
              deciding sense of direction is in P. Our algorithms can also be used to decide in AC<sup>1</sup> whether a coloured
              graph is a Cayley colour graph.</p>              
              }
}

						
