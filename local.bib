	@inproceedings{VigBIRSQ,
  author    = {Sebastiano Vigna},
  title     = {Broadword Implementation of Rank/Select Queries},
  publisher = SV,
  year      = 2008,
  editor    = {McGeoch, Catherine C.},
  booktitle = {Experimental Algorithms. 7th International Workshop, WEA 2008},
  series    = LNCS,
  number    = 5038,
  pages     = {154--168},
  category  = CONVEGNO,
  notextra  = {               
               <p>Research on succinct data structures (data structures occupying space close to
               the information-theoretical lower bound, but achieving speed similar to their
               standard counterparts) has steadily increased in the last few years. However,
               many theoretical constructions providing asymptotically optimal bounds are
               unusable in practice because of the very large constants involved. The study
               of practical implementations of the basic building blocks of such data
               structures is thus fundamental to obtain practical applications. In this paper
               we argue that 64-bit and wider architectures are particularly suited to very
               efficient implementations of rank (counting the number of ones up to a given position) 
               and select (finding the position of the <var>i</var>-th bit set), two essential building
               blocks of all succinct data structures. Contrarily to typical 32-bit
               approaches, involving precomputed tables, we use pervasively <em>broadword</em>
               (a.k.a. SWAR&mdash;&ldquo;SIMD in A Register&rdquo;) programming, which compensates the
               constant burden associated to succinct structures by solving problems in parallel
               in a register. We provide an implementation named <code>rank9</code> that addresses
               2<sup>64</sup> bits, consumes less space and is significantly faster then current
               state-of-the-art 32-bit implementations, and a companion <code>select9</code>
               structure that selects in nearly constant time using only access to
               aligned data. For sparsely populated arrays, we provide a simple broadword
               implementation of the Elias&ndash;Fano representation of monotone sequences. In doing
               so, we develop broadword algorithms to perform selection in a word 
               or in a sequence of words that are of independent interest.</p>
               <p><b><a href="https://vigna.di.unimi.it/ftp/papers/Broadword.pdf">PDF version</a>; a complete C++ and Java&trade; implementation is available at the <a href="https://sux.di.unimi.it/">Sux project home page</a>.</b>
               }
}

	@techreport{VigBIPQ,
  author   = {Sebastiano Vigna},
  title    = {Broadword Implementation of Parenthesis Queries},
  type     = {Arxiv preprint arXiv:1301.5468},
  year     = 2013,
  notextra = {
              <p>We continue the line of research started in &ldquo;Broadword Implementation
              of Rank/Select Queries&rdquo; proposing broadword (a.k.a. SWAR&mdash;&ldquo;SIMD Within A
              Register&rdquo;) algorithms for finding matching closed parentheses and the <var>k</var>-th
              far closed parenthesis. Our algorithms work in time <i>O</i>(log&nbsp;<var>w</var>) on a word of
              <var>w</var> bits, and contain no branch and no test instruction. On 64-bit (and
              wider) architectures, these algorithms make it possible to avoid costly
              tabulations, while providing a very significant speedup with respect to
              for-loop implementations.</p>
              <p><b><a href="https://vigna.di.unimi.it/ftp/papers/Parenthesis.pdf">PDF version</a>; a complete C++ and Java&trade; implementation is available at the <a href="https://sux.di.unimi.it/">Sux project home page</a>.</b>
              }
}


