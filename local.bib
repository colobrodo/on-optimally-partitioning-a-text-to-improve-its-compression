	@article{BoVCPSD,
  author   = {Paolo Boldi and Sebastiano Vigna},
  title    = {Coverings That Preserve Sense of Direction},
  journal  = IPL,
  volume   = 75,
  year     = 2000,
  pages    = {175--180},
  notextra = {
              <p>Sense of direction is a property of labelled networks (i.e., arc-coloured
              graphs) that allows one to assign coherently local identifiers to other
              processors on the basis of the route followed by incoming messages. We prove
              that (weak) sense of direction is preserved by the construction of regular
              coverings (i.e., coverings induced by voltage assignments in a group) whose
              voltage assignment depends only on colours. Moreover, this construction
              preserves minimality.</p>              
              },
  category = journal
}

