	@article{BoVLBWSD,
  author   = {Paolo Boldi and Sebastiano Vigna},
  title    = {Lower Bounds for Weak Sense of Direction},
  volume   = 1,
  journal  = {J. Discrete Algorithms},
  year     = 2003,
  pages    = {119--128},
  category = journal,
  notextra = {
              <p>A graph with <var>n</var> and maximum degree &#x0394; cannot be given weak
              sense of direction using less than &#x0394; colours. It is known that <var>n</var>
              colours are always sufficient, but it has been conjectured that just &#x0394;+1
              are really needed. On the contrary, we show that for sufficiently large <var>n</var>
              there are graphs requiring &#x0394;+&#x03c9;(<var>n</var>/log <var>n</var>) colours. We also give
              simple examples of small graphs requiring &#x0394;+2 colours, which have been
              verified mechanically.</p>              
              }
}


	@inproceedings{BoVMLBSD,
  author    = {Paolo Boldi and Sebastiano Vigna},
  title     = {More Lower Bounds for Weak Sense of Direction: The Case of Regular Graphs},
  publisher = SV,
  booktitle = {Distributed Computing. 14th International Conference, DISC 2000},
  address   = {Toledo, Spain},
  series    = LNCS,
  pages     = {238--252},
  volume    = 1914,
  year      = 2000,
  category  = CONVEGNO,
  notextra  = {               
               <p>A graph <var>G</var> with <var>n</var> vertices and maximum degree &#x0394;<sub><var>G</var></sub> cannot be given
               weak sense of direction using less than &#x0394;<sub><var>G</var></sub> colours.  It is known that
               <var>n</var> colours are always sufficient, and it was conjectured that just
               &#x0394;<sub><var>G</var></sub>+1 are really needed, that is, one more colour is
               sufficient. Nonetheless, it has just been shown that for
               sufficiently large <var>n</var> there are graphs requiring &#x03c9;(<var>n</var>/log <var>n</var>) more
               colours than &#x0394;<sub><var>G</var></sub>. In this paper, using recent results in asymptotic graph enumeration,
               we show not only that (somehow surprisingly) the same bound holds for regular
               graphs, but also that it can be improved to &#x03a9;(<var>n</var> log log <var>n</var>/log <var>n</var>) 
               We
               also show that &#x03a9;(d<sub><var>G</var></sub>(log log d<sub><var>G</var></sub>)<sup>1/2</sup>) colours are
               necessary, where d<sub><var>G</var></sub> is the degree of <var>G</var>.</p>
               <p><b><a href="https://vigna.di.unimi.it/ftp/papers/MoreLowerBoundsWSoD.pdf">PDF version</a>.</b>
               }
}



