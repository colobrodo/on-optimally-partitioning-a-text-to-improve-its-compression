@article{BoVEIT,
  author   = {Boldi, Paolo and Vigna, Sebastiano},
  title    = {{$E=I+T$}: {T}he Internal Extent Formula for Compacted Tries},
  journal  = IPL,
  year     = 2011,
  volume   = 111,
  pages    = {310--313},
  notextra = {
              <p>It is well known that in a binary tree the
              external path length minus the internal path length
              is exactly 2<var>n</var>&nbsp;&minus;&nbsp;2, where <var>n</var> is the number of external nodes. We show that a
              generalization of the formula holds for compacted tries, replacing the
              role of <em>paths</em> with the notion of <em>extent</em>, and the value 2<var>n</var>&nbsp;&minus;&nbsp;2 with the <em>trie measure</em>,
              an estimation of the number of bits that are necessary to
              describe the trie. 
              </dd>
              <p><b><a href="https://vigna.di.unimi.it/ftp/papers/EIT.pdf">PDF version</a>; <a href="https://doi.org/10.1016/j.ipl.2010.12.012">doi.org:10.1016/j.ipl.2010.12.012</a>.</b>
